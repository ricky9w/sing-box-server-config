{
  "inbounds": [
    {
      "type": "trojan",
      "listen": "::",
      "listen_port": 443,
      "users": [
        {
          "name": "<your-name>",
          "password": "<your-password>"
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "<your-domain>",
        "alpn": ["h2", "http/1.1"],
        "min_version": "1.2",
        "acme": {
          "domain": "<your-domain>",
          "email": "<email-for-acme>"
        }
      },
      "multiplex": {
        "enabled": true,
        "padding": true
      },
      "sniff": true,
      "sniff_override_destination": true
    }
  ]
}
